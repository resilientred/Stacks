    <script>function prevIconName
	global gCurrIconIndex
	put "badman25" into outIconName
	if gCurrIconIndex &gt; 29 and gCurrIconIndex &lt; 39 then
		return "badman" &amp; (((gCurrIconIndex +(46 +31)) mod 46) +1)
	end if
	return outIconName
end prevIconName

function prevTile theTile
	if item 2 of theTile &lt; 15 then
		put (item 2 of theTile +1) into item 2 of theTile
		return theTile
	else
		return ""
	end if
end prevTile

function nextTile theTile
	if item 2 of theTile &gt; 1 then
		put (item 2 of theTile -1) into item 2 of theTile
		return theTile
	else
		return ""
	end if
end nextTile

function nextIconName
	global gCurrIconIndex
	put "badman25" into outIconName
	if gCurrIconIndex &gt; 10 and gCurrIconIndex &lt; 30 then
		return "badman" &amp; (((gCurrIconIndex +(46 +13)) mod 46) +1)
	end if
	return outIconName
end nextIconName


on idle
	global gCurrTile
	global gCurrIconIndex
	
	if gCurrTile is empty then
		put "1,15" into gCurrTile
		put 1 into gCurrIconIndex
	else
		add 1 to gCurrIconIndex
		if gCurrIconIndex &gt; 46 then
			put 1 into gCurrIconIndex
		else if gCurrIconIndex is 17 then
			if item 2 of gCurrTile is 1 then
				set icon of cd btn gCurrTile to "badman25"
				put 15 into item 2 of gCurrTile
			else
				subtract 1 from item 2 of gCurrTile
			end if
			put 30 into gCurrIconIndex
		end if
	end if
	put prevTile(gCurrTile) into prevTile
	put nextTile(gCurrTile) into nextTile
	if prevTile is not empty then
		set icon of cd btn prevTile to prevIconName()
	end if
	if nextTile is not empty then
		set icon of cd btn nextTile to nextIconName()
	end if
	set icon of cd btn gCurrTile to "badman" &amp; gCurrIconIndex
	
	--pass idle
end idle</script>
